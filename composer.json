{
    "name": "nemrutco/nova-filterable-metrics",
    "description": "Filterable Laravel Nova Metrics.",
    "keywords": [
        "laravel",
        "nova",
        "filterable",
        "metrics",
        "card"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Nemrutco\\Filterable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nemrutco\\Filterable\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
